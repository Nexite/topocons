{
  "name": "@codeday/topocons",
  "version": "2.0.0",
  "description": "SRND icons for React projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Tyler Menezes",
  "license": "UNLICENSED",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf src/Icon && rm -rf dist",
    "build": "tsc",
    "pregenerate-icons": "svgo -f ./src/svg/Icon -o src/optimized-svg/Icon",
    "generate-icons": "mkdir -p src/Icon && create-chakra-icons -o src/Icon src/optimized-svg/Icon --typescript --multi -T C && idxgen src/Icon/",
    "postgenerate-icons": "replace '#000000' 'currentColor' src/Icon/*.tsx && replace '#000' 'currentColor' src/Icon/*.tsx"
  },
  "devDependencies": {
    "@chakra-ui/react": "^2.5.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@nexite/create-chakra-icons": "^1.1.1",
    "@types/node": "^16.11.6",
    "@types/react": "^18.0.0",
    "auto-export-components": "^1.0.5",
    "framer-motion": "^10.0.1",
    "idxgen": "^1.3.1",
    "react": "^18.2.0",
    "replace": "^1.2.2",
    "svgo": "^3.0.2",
    "ts-create-index": "^3.0.10",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^2.5.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "framer-motion": "^10.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
